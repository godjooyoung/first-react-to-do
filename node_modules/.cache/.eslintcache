                   isDone: false,\n                    title: '',\n                    body: ''\n                },\n                todoList : [...state.todoList, state.todo]\n            }\n        case DELETE_TODO:       \n            state.todoList = state.todoList.filter((x)=>{\n                return x.id != action.payload\n            })\n            return {\n                todo : {\n                    id: state.todo.id,\n                    isDone: false,\n                    title: '',\n                    body: ''\n                },\n                todoList : state.todoList\n            }\n        case UPDATE_TODO:\n            return {\n                todo : {\n                    id: state.todo.id,\n                    title: action.payload.title,\n                    body: action.payload.body,\n                    isDone : false\n                },\n                todoList : state.todoList\n            }\n        case TOGGLE_TODO:\n            state.todoList.map((x) => {\n                    if(x.id == action.payload){\n                        x.isDone = !x.isDone\n                    }\n                }\n            );\n            return {\n                todo : {\n                    id: state.todo.id,\n                    isDone: false,\n                    title: '',\n                    body: ''\n                },\n                todoList : [...state.todoList]\n            }\n        case SELECT_TODO:\n            // TODO 상세보기, 상세보기 하면서 상태값 변경 (리스트는 그대로 입력값은 초기값으로\n            return {\n                todo : {\n                    id: state.todo.id,\n                    isDone: false,\n                    title: '',\n                    body: ''\n                },\n                todoList : state.todoList\n            }\n\n        default:\n            return {\n                todo : {\n                    id: state.todo.id,\n                    isDone: false,\n                    title: '',\n                    body: ''\n                },\n                todoList : []\n            }\n\n\n            \n            \n    }\n}\n\nexport default todoReducer","/Users/jooyoungshin/first-react-to-do/src/redux/config/configStore.js",[],[],"/Users/jooyoungshin/first-react-to-do/src/shared/Router.jsx",[],[],"/Users/jooyoungshin/first-react-to-do/src/pages/Details.js",["83"],[],{"ruleId":"84","severity":1,"message":"85","line":42,"column":50,"nodeType":"86","messageId":"87","endLine":42,"endColumn":52},{"ruleId":"88","severity":1,"message":"89","line":44,"column":17,"nodeType":"90","messageId":"91","endLine":44,"endColumn":25},{"ruleId":"84","severity":1,"message":"92","line":46,"column":20,"nodeType":"86","messageId":"87","endLine":46,"endColumn":22},{"ruleId":"93","severity":1,"message":"94","line":49,"column":25,"nodeType":"95","messageId":"96","endLine":49,"endColumn":36},{"ruleId":"84","severity":1,"message":"92","line":3,"column":19,"nodeType":"86","messageId":"87","endLine":3,"endColumn":21},{"ruleId":"84","severity":1,"message":"92","line":8,"column":44,"nodeType":"86","messageId":"87","endLine":8,"endColumn":46},{"ruleId":"84","severity":1,"message":"92","line":26,"column":44,"nodeType":"86","messageId":"87","endLine":26,"endColumn":46},{"ruleId":"84","severity":1,"message":"92","line":76,"column":27,"nodeType":"86","messageId":"87","endLine":76,"endColumn":29},{"ruleId":"84","severity":1,"message":"92","line":76,"column":45,"nodeType":"86","messageId":"87","endLine":76,"endColumn":47},{"ruleId":"97","severity":1,"message":"98","line":84,"column":11,"nodeType":"99","messageId":"100","endLine":84,"endColumn":19},{"ruleId":"97","severity":1,"message":"101","line":85,"column":11,"nodeType":"99","messageId":"100","endLine":85,"endColumn":16},{"ruleId":"84","severity":1,"message":"92","line":93,"column":33,"nodeType":"86","messageId":"87","endLine":93,"endColumn":35},{"ruleId":"84","severity":1,"message":"92","line":93,"column":64,"nodeType":"86","messageId":"87","endLine":93,"endColumn":66},{"rul